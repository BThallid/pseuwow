#permission=255
// example script to explain how to use loops/ifs

set,x 0
loop
	set,y 0
	add,x 1		
	if ?{bigger,${x} 5}
		exitloop
	endif
	loop
		if ?{bigger,${y} 8}
			exitloop
		else
			set,tmp ${x}
			mul,tmp ${y}
			out LOOP:${x}:${y} = ${tmp}
		endif
		add,y 1
	endloop
endloop
out end if/loop test script